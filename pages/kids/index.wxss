/* pages/kids/index.wxss */
.kids_pg {
  height: 90vh;
  width: 100vw;
  background: #f2f2f2;
}
.overflow {
  overflow: hidden;
  height: 90vh;
  width: 100vw;
}
.kid_item {
  width: 100vw;
  margin-top: 15rpx;
  height: 200rpx;
  position: relative;
}
.kid_item .item_box {
  background-color: #fff;
  display: flex;
  height: 200rpx;
  position: absolute;
  width: 100vw;
}
.del_box {
  position: absolute;
  width: 150rpx;
  height: 200rpx;
  background-color: red;
  color: #fff;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 50rpx;
  right: -150rpx;
}
.img_box {
  flex: 2;
  height: 100%;
}
.img_box .img {
  width: 100%;
  transform: scale(0.8);
}
.desc_edit_box {
  flex: 6;
  display: flex;
  justify-content: space-around;
  padding-top: 20rpx;
}
.desc_edit_box .desc_box {
  display: flex;
  flex-direction: column;
  font-size: 30rpx;
}
.bottom_tool  {
  background-color: #d64f6f;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 40rpx;
  height: 10vh;
  color: #fff;
  width: 100vw;
  position: relative;
}
.edit_dialog {
  background: rgba(0,0,0,.2);
  width: 100%;
  height: 100vh;
  position: absolute;
  top: 0;
  left: 0;
  z-index: 999;
}
.edit_box {
  height: 400rpx;
  background-color: #fff;
  border-radius: 20rpx 20rpx 0 0;
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  padding: 15rpx 20rpx;
}
.closeDialog {
  position: absolute;
  top: 15rpx;
  right: 15rpx;
  font-size: 30rpx;
  color: grey;
}
.edit_item {
  display: flex;
  font-size: 30rpx;
  padding: 40rpx;
  height: 80rpx;
  align-items: center;
}

.add_btn {
  border: 2rpx solid #d64f6f;
  background: transparent;
  margin: 0rpx 50rpx;
  padding: 0rpx;
  color: #d64f6f;
}
button::after {
  content: none;
}
.iconfont {
  font-size: 50rpx;
}
radio.female .wx-radio-input {
  appearance: none;
  width: 80rpx;
  height: 80rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: cover!important;
  outline: none!important;
  border: none!important;
}

radio.female .wx-radio-input.wx-radio-input-checked::before {
  content: '';
  appearance: none!important;
  width: 110rpx;
  height: 110rpx;
  background-size: cover!important;
  border: none!important;
  outline: none!important;
  background: url("data:image/png;base64,") no-repeat;
}
radio.female .wx-radio-input.wx-radio-input-checked {
  background: none!important;
}
radio.male .wx-radio-input {
  appearance: none;
  width: 100rpx;
  height: 100rpx;
  background: url("data:image/png;base64,") no-repeat;
  background-size: cover!important;
  outline: none!important;
  border: none!important;
}
radio.male .wx-radio-input.wx-radio-input-checked {
  background: none;
}
radio.male .wx-radio-input.wx-radio-input-checked::before {
  content: '';
  appearance: none!important;
  width: 135rpx;
  height: 135rpx;
  background-size: cover!important;
  border: none!important;
  outline: none!important;
  background: url('data:image/png;base64,') ;
}
radio.male .wx-radio-input.wx-radio-input-checked .female .wx-radio-input {
  background-image: none!important;
} 